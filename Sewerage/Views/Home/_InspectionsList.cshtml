<div data-bind="if: $root.ChosenSectionId() != null">
    <table class="observationTable">
        <tr>
            <thead>
                <th>Start date</th>
                <th>End date</th>
                <th>Video Url</th>    
            </thead>
            <tbody data-bind="foreach: Inspections">
                <tr data-bind="css: { selected: InspectionId == $parent.ChosenInspectionId() }, click: $parent.selectInspection">
                    <td data-bind="text: StartDate"></td>
                    <td data-bind="text: EndDate"></td>
                    <td data-bind="text: VideoUrl"></td>
                </tr>
            </tbody>
        </tr>
    </table>
</div>
